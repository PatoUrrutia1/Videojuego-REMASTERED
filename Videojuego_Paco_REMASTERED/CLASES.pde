class personaje {
  // Atributos 
  String name; 
  int hp;
  int attack;
  int defense;
    
  // Parámetros del método de consturcción de clase.
  personaje(String name_, int hp_, int attack_, int defense_){
    
    // Parámetros del método de construcción se asignan como valores de atributos.
    name = name_;
    hp = hp_;
    attack = attack_;
    defense = defense_;
        
  }
}

class Batman1 extends personaje {
  Batman1 (String name_, int hp_, int attack_, int defense_){
    super (name_, hp_, attack_, defense_);
  }
}

class Batman2 extends personaje {
  Batman2 (String name_, int hp_, int attack_, int defense_){
    super (name_, hp_, attack_, defense_);
  }
}

